<div class="main row">
	<!-- Main Post -->
	<div class="col-xs-12 col-sm-8">
		<%= render "shared/channel_nav" %>
		<% if params[:query] %>
			<%= render "search" %>
		<% elsif params[:tag] %>
			<%= render "tag" %>
		<% else %>
			<%= render "category" %>
		<% end %>
	</div>

	<!-- Side Bar -->
	<div class="hidden-xs col-sm-4">
		<%= render "shared/sidebar" %>
	</div>
</div>